QWidget#ChatForm{
    background: url("images/Chat.jpg") no-repeat

}
QLabel#f_avatar_label{
    border-radius: 50%;
}
QLabel#my_avatar_label{
    border-radius: 50%;
}
QLabel#f_avatar_label:hover,QLabel#my_avatar_label:hover{
    border: 2px solid #fff;
}
QLabel#f_nickname_label{
    font-size: 24px;
    font-weight: bold;
    color: #818990;
}
QLabel#address_label,QLabel#birthday_label,QLabel#email_label,QLabel#tel_label{
    font-size: 16px;
    font-weight: bold;
    color: #D7DCE2;
}
QLabel#style_label{
    font-size:16px;
    color: #818990;
}
QLabel#chatname_label{
    font-size: 24px;
    font-weight: bold;
    color: #AAE0DB;
}
QLabel#onlinestatus_label{
    font-size: 16px;
    font-weight: bold;
    color: #818990;
}
QTextBrowser#textBrowser{
    border: none;
    font-size: 20px;
    color: #58C9FF;
}
QTextEdit#textEdit{
    border: none;
    font-size: 16px;
}

QPushButton#send_Button{
